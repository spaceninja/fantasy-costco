    "@vitejs/plugin-vue": "5.2.1",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "@vue/tsconfig": "0.7.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-vue": "9.32.0",
    "npm-run-all2": "7.0.2",
    "postcss-html": "1.8.0",
